					m_nNodeID = 
					{
						m_id = 1228302265
					}
					m_networkMode = "ServerAuthoritative"
					m_sNote = ""
					m_commentText = "This state machine is set up so that the start state will never be current: it transitions to another state as soon as the state machine updates for the first time after getting reset.  All the transitions have a blend time of 0, so the start state is never used as part of the blend of the character\'s pose, which is why its safe to have nothing connected to it.  \n\nThe rest of the states are marked as End States.  This means that when nodes connected to these states are \"finished\", that the state machine will report to its parent that it is \"finished\" as well.  We\'ve set it up this way so that the Movement state machine can use the Finished condition to know when the turn is done."
					m_size = [ 600.0, 467.0 ]
					m_color = [ 100, 149, 237, 255 ]
